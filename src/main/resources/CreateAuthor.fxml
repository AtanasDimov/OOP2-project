<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="230.0" prefWidth="396.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarysoftware.CreateAuthorController">
   <children>
      <TextField fx:id="CreateAuthor_txtName" layoutX="51.0" layoutY="50.0" promptText="Име" />
      <TextField fx:id="CreateAuthor_txtDescription" layoutX="51.0" layoutY="102.0" promptText="Описание" />
      <Button fx:id="CreateAuthor_btnSubmit" layoutX="60.0" layoutY="166.0" mnemonicParsing="false" onAction="#Submit" text="Запази" />
      <Button fx:id="CreateAuthor_btnClear" layoutX="126.0" layoutY="166.0" mnemonicParsing="false" onAction="#Clear" text="Изчисти" />
      <Button fx:id="CreateAuthor_btnBack" layoutX="236.0" layoutY="166.0" mnemonicParsing="false" onAction="#Back" text="Назад" />
      <ComboBox fx:id="CreateAuthor_ComboBox" layoutX="51.0" layoutY="14.0" prefWidth="150.0" promptText="Вид Автор" />
   </children>
</AnchorPane>
